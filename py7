# Finds the nth prime
import math

def primeSieve(target):
    index = 2
    primes = list()
    sieve = [True for i in range(target**2)]
    while index <= target:
        if sieve[index] == True:
            for i in range(index * 2, target**2, index):
                sieve[i] = False
        index += 1

    primeCounter = 0    
    targetReached = False     
    for i in range(2, target**2):
        if sieve[i] == True:
            primes.append(i)
            primeCounter += 1
            if primeCounter == target:
                break
    return primes

#print(primeSieve(10001)[-1])
print(primeSieve(10000))
