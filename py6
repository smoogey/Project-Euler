# Finds the difference between the sum of the squares and the square of the sum

def sumOfSquares(num):
    sum = 0
    for i in range(1,num + 1):
        sum = sum + i**2
    return sum

def squareOfSum(num):
    sum = 0
    for i in range(1,num + 1):
        sum = sum + i
    sum = sum**2
    return sum

def difference(num):
    diff = squareOfSum(num) - sumOfSquares(num)
    return diff

print(difference(100))
